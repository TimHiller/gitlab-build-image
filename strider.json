{
	"merge_plugins": true,
	"plugins": [
		{
			"id": "node",
			"enabled": true
		},
		{
			"id": "custom",
			"enabled": true,
			"config": {
				"shell": "bash",
				"prepare": {
					"screen": "Logging into Docker Hub...",
					"command": "docker login --username $DOCKER_USERNAME --password $DOCKER_PASSWORD"
				}
			}
		}
	]
}
